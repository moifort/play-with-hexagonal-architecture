{
  "extends": [
    "eslint:recommended",
    "angular"
  ],
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "wrap-iife": [2, "inside"],
    "eqeqeq": 2,
    "no-use-before-define": [2, "nofunc"],
    "no-unused-vars": 2,
    "no-multi-str": 2,
    "no-irregular-whitespace": 2,
    "semi": [2, "always"],
    "indent": 2,
    "linebreak-style": 0,
    "eol-last": 2
  }
}
